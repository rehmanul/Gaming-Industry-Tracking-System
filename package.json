{
  "name": "gaming-industry-tracker",
  "version": "3.0.0",
  "description": "Production-grade gaming industry hiring and job tracking system with advanced analytics",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "web": "node src/web/index.js",
    "dev": "concurrently \"npm run web\" \"npm run start\"",
    "test": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t gaming-tracker .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop gaming-tracker",
    "pm2:restart": "pm2 restart gaming-tracker",
    "logs": "pm2 logs gaming-tracker",
    "health-check": "node scripts/health-check.js",
    "deploy-check": "node scripts/deploy-check.js",
    "analytics": "node scripts/analytics-report.js",
    "export-data": "node scripts/export-data.js",
    "backup": "node scripts/backup-data.js",
    "performance-test": "node scripts/performance-test.js"
  },
  "keywords": [
    "gaming",
    "recruitment",
    "job-tracking",
    "hiring",
    "analytics",
    "automation",
    "web-scraping"
  ],
  "author": "Gaming Industry Tracker Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "puppeteer": "^21.5.2",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "googleapis": "^128.0.0",
    "nodemailer": "^6.9.7",
    "ws": "^8.14.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "chart.js": "^4.4.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaming-tracker/gaming-industry-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/gaming-tracker/gaming-industry-tracker/issues"
  },
  "homepage": "https://github.com/gaming-tracker/gaming-industry-tracker#readme"
}